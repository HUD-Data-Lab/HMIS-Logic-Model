@prefix : <http://example.org/hmis#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:Client a owl:Class .
:Enrollment a owl:Class .
:Project a owl:Class .
:ProjectType a owl:Class .
:ResponseOption a owl:Class .

:hasEnrollment a owl:ObjectProperty ; rdfs:domain :Client ; rdfs:range :Enrollment .
:enrolledInProject a owl:ObjectProperty ; rdfs:domain :Enrollment ; rdfs:range :Project .
:hasProjectType a owl:ObjectProperty ; rdfs:domain :Project ; rdfs:range :ProjectType .
:hasSex a owl:ObjectProperty ; rdfs:domain :Client ; rdfs:range :ResponseOption .
:hasVeteranStatus a owl:ObjectProperty ; rdfs:domain :Client ; rdfs:range :ResponseOption .
:hasStartDate a owl:DatatypeProperty ; rdfs:domain :Enrollment ; rdfs:range xsd:date .
:hasExitDate a owl:DatatypeProperty ; rdfs:domain :Enrollment ; rdfs:range xsd:date .
:hasAge a owl:DatatypeProperty ; rdfs:domain :Client ; rdfs:range xsd:integer .

:ES a :ProjectType .
:RRH a :ProjectType .
:DayShelter a :ProjectType .

:Male a :ResponseOption .
:Female a :ResponseOption .
:Man a :ResponseOption .
:Woman a :ResponseOption .
:Yes a :ResponseOption .
:No a :ResponseOption .

:Male owl:sameAs :Man .
:Female owl:sameAs :Woman .

:VeteranStatus a owl:DeprecatedClass .
